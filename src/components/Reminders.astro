---
import CardReminders from "./CardReminders.astro"

const reminders = [
  { emoji: '💧', title: 'Beber agua', interval: 40 },
  { emoji: '🙆‍♂️', title: 'Estirarse', interval: 20 },
  { emoji: '🧘', title: 'Respirar profundo', interval: 30 },
  { emoji: '🚶‍♀️', title: 'Mover las piernas', interval: 150 }
];
---

<ul>
    {reminders.map(reminder => <CardReminders emoji={reminder.emoji} title={reminder.title} interval={reminder.interval}/>)}
</ul>